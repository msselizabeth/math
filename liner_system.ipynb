   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "r1 = np.array([2,1,5])\n",
    "r2 = np.array([1,3,5])\n",
    "# A = np.array([r1,r2])\n",
    "\n",
    "r2 = r2 - 0.5 * r1\n",
    "\n",
    "A = np.array([r1,r2])\n",
    "print(A)\n",
    "y = r2[2] / r2[1]\n",
    "x = (r1[2] - r1[1]*y) / r1[0]\n",
    "\n",
    "print(f\"x: {x}, y: {y}\")"
   ]
  }
